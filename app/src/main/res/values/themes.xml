<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Care4Pets" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/fondo</item>
        <item name="colorPrimaryDark">#B6AD08</item>
        <item name="colorAccent">@color/black</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Care4Pets.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Care4Pets.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Care4Pets.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>